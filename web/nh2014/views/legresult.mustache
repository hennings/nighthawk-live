{{> header }}

<div class="container">
<h2 id="heading">Leg results {{class}} at leg {{leg}}</h2>

{{> overview }}

<table class="table table-striped table-bordered table-responsive table-condensed">
  <tr>
   <th>rank</th>
   <th>team bib</th>
   <th>runner</th>
   <th>time</th>
   <th class="time-behind"></th>
   <th>club</th>
</tr>

{{#res}}

 {{#RestartFlag}} 
 <tr><td colspan="100%">Restart {{RestartFlag}}</td></tr>
 {{/RestartFlag}}

 <tr>
  <td>{{Rank}}</td>
  <td><a href="{{base}}/{{raceId}}/{{class}}/team/{{TeamId}}">{{TeamId}}</a></td>
  <td>{{Name}}</td>
  <td>{{Timestr}}</td>
  <td class="time-behind">{{Behind}}</td> 
  <td>{{Club}}</td>
 </tr>
{{/res}}

</table>


</div>



{{> footer }}

